//slider customer
.section.section--customer
	.section-title Наши заказчики
	.main-cont
		.slider.owl-carousel
			- for (var i=1;i<15;i++)
				.slider__el
					img(src="img/"+i+"-customer.jpg", alt="")

//slider certificate
.section.section--certificate
	.filter
	.section-title Наши сертификаты
	.main-cont
		.slider.owl-carousel
			- for (var i=1;i<15;i++)
				.slider__el
					a(href="img/cert.jpg" rel="group-elemen").fancybox
						img(src="img/cert.jpg", alt="")

