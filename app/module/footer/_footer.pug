mixin footer()
	//block footer
	//.section.section--footer
		.main-cont
			.section-title Оформите заказ
			.section-title-sub Прием в офисе с понедельника по пятницу с 9:00 до 20:00, суббота: по предварительной записи, выходной: воскресенье.
			.footer__wrap
				.footer__el
					.footer__icon
						+icon("map","icon")
					.footer__title В офисе
					.footer__text г. Москва, 1-ый Варшавский проезд, д.1А, строен.9, офис 2.6
				.footer__el
					.footer__icon
						+icon("phone-call","icon")
					.footer__title По телефону
					a(href="tel:+7 (495) 514-84-13").footer__text.footer__text--red +7 (495) 514-84-13
					a(href="tel:+7 (495) 283-96-83").footer__text.footer__text--red +7 (495) 283-96-83
				.footer__el
					.footer__icon
						+icon("mail","icon")
					.footer__title По email
					a(href="mailto:info@cgiku.ru").footer__text.footer__text--red info@cgiku.ru
			.footer-get
				.footer-get__el.modal-get(data-modal="order") Оставить заявку

	//.footer-map
		h1 Карта


	//section-contact
	#contact.section.section-contact
		.main-cont
			.section-title Наши контакты
			.contact
				.contact-form.contact-form--footer
					.main-form
						.form-title Оставьте вашу заявку
						.form-title-sub Мы свяжемся с вам в кратчайшее время
						form.validate-form.form--footer( action="javascript:void(null);")
							.input-row
								input(type="text" name="name" placeholder="Ваше имя").input
							.input-row
								input(type="text" name="phone" placeholder="Ваш телефон").input
							.input-row
								textarea(name="info", cols="30", rows="5" placeholder="Сообщение").input
							.agree-row
								.check-el
									label.style-label.style-label--checkbox
										input(type="checkbox" name="agree").sytle-input
										.sytle-input-element
											+icon("checked","icon-check")
										.sytle-input-text=text
								.agree-row__text Согласен с
									a(href="#") условиями обработки персональных данных
							input(type="submit" value="Отправить заявку").form-footer-send
					.contact-info
						.map
							script("text/javascript" charset="utf-8" async src="https://api-maps.yandex.ru/services/constructor/1.0/js/?um=constructor%3A104108c83896b1a6c1b544e50f3d690ee4df2f43b13730f0b9a6f0d5ead96cae&amp;width=100%&amp;height=400&amp;lang=ru_RU&amp;scroll=false")

						.contact-row-wrap
							.contact-row
								.contact-icon
									+icon("placeholder","icon")
								.contact-text Москва, 1-ый Варшавский проезд, д.1А, строен.9, офис 2.6
							.contact-row
								.contact-icon
									+icon("phone-call","icon")
								.contact-text +7 (495) 514-84-13
							.contact-row
								.contact-icon
									+icon("mail","icon")
								.contact-text info@cgiku.ru
							.contact-row
								.contact-icon
									+icon("metro","icon")
								a(href="#").contact-text Как к нам проехать подробнее
	//section-contact-end

	.footer
		.footer-menu
			.main-cont
				.menu
					.menu-el
						a(href="#")
							span Главная
					.menu-el.menu-el-service
						a(href="#")
							span Услуги
					.menu-el
						a(href="#")
							span О компании
					.menu-el
						a(href="#")
							span Прайс-лист
					.menu-el
						a(href="#")
							span Наши лицензии
					.menu-el
						a(href="#")
							span Контакты
					.menu-el.menu-el--mark
						a(href="#")
							span Акции
		.footer-info
			.main-cont
				.footer-info-wrap
					a(href="/").footer-info__logo
						img(src="img/logo-dark.png", alt="").logo--full
					.footer-info__phone
							a(href="tel:+7 (495) 514-84-13").footer-info__phone +7 (495) 514-84-13
							a(href="tel:+7 (495) 514-84-13").footer-info__phone +7 (495) 283-96-83

	.scroll-up
		+icon("arrow-up","icon")

	.modal-layer
		.modal-cont
			.modal-filter
			.modal-wrap
				.modal(data-modal="order")
					.modal-content.modal-content--fogot
						.modal-close
							+icon("close","icon")
						.modal-title Заявка
						.modal-text Оформите обращение с помощью этой формы, мы перезвоним Вам и проконсультируем по любому Вашему вопросу совершенно бесплатно!
						form.validate-form( action="javascript:void(null);")
							.input-row
								input(type="text" name="name" placeholder="Ваше имя").input
							.input-row
								input(type="text" name="phone" placeholder="Ваш телефон").input
							.input-row
								textarea(name="info", cols="30", rows="5" placeholder="Сообщение").input
							.agree-row
								.check-el
									label.style-label.style-label--checkbox
										input(type="checkbox" name="agree").sytle-input
										.sytle-input-element
											+icon("checked","icon-check")
										.sytle-input-text=text
								.agree-row__text Согласен с
									a(href="#") условиями обработки персональных данных
							input(type="submit" value="Отправить заявку").modal-send

				.modal(data-modal="true")
					.modal-content.modal-content--true
						+icon("interface","icon")
						.modal-text-true Ваша заявка принята

				.modal(data-modal="next")
					.modal-content.modal-content--truesend
						+icon("interface","icon")
						.modal-text-truesend Ваша заявка принята!

				.modal(data-modal="sale").modal--sale
					.modal-content
						.modal-close
							+icon("close","icon")
						.modal-sale__title Закажите больше одной услуги и получите скидку:
						.modal-sale__wrap
							.modal-sale__list
								.modal-sale__el
									.modal-sale__el-icon
										+icon("gift--color","icon")
									.modal-sale__el-text Технический  план строения до 100 кв.м. (14 000 руб.) и межевой план земельного участка до 8 соток (14 000 руб.)
										.modal-sale__el-text-sub Общая стоимость
											span.main-service__price--false 28 000
											span.main-service__price--true 24 000 руб.
											|- Скидка 4000 руб.
								.modal-sale__el
									.modal-sale__el-icon
										+icon("gift--color","icon")
									.modal-sale__el-text Технический  план строения до 100 кв.м. (14 000 руб.)  и технический  план строения до 50 кв.м. (10 000 руб.)
										.modal-sale__el-text-sub Общая стоимость
											span.main-service__price--false 28 000
											span.main-service__price--true 24 000 руб.
											| - Скидка 4000 руб.
							.modal-sale__text
								p Объединитесь с соседями! При единовременном заказе на межевание или технические планы соседних участков у нас гарантированно предоставляются скидки.  Размер скидки зависит от объёма необходимых работ. <br> При повторном обращении — скидки от 10%. <br>
									span За подробностями обращайтесь по телефону!

							.modal-sale__text-add Внимание! На услуги стоимостью 10 000 руб. и меньше скидки не распространяются!
							.row-btn--center
								.modal-sale-get.modal-get(data-modal="order") Заказать

				.modal(data-modal="sale--border").modal--sale
					.modal-content
						.modal-close
							+icon("close","icon")
						.modal-sale__title Закажите больше одной услуги и получите скидку:
						.modal-sale__wrap
							.modal-sale__list
								.modal-sale__el
									.modal-sale__el-icon
										+icon("gift--color","icon")
									.modal-sale__el-text Межевой план земельного участка до 8 соток (14 000 руб.) и  технический  план строения до 100 кв.м.(14 000 руб.)
										.modal-sale__el-text-sub Общая стоимость
											span.main-service__price--false 28 000
											span.main-service__price--true 24 000 руб.
											| - Скидка 4000 руб.
							.modal-sale__text
								p Объединитесь с соседями! При единовременном заказе на межевание или технические планы соседних участков у нас гарантированно предоставляются скидки.  Размер скидки зависит от объёма необходимых работ. <br> При повторном обращении — скидки от 10%. <br>
									span За подробностями обращайтесь по телефону!

							.modal-sale__text-add Внимание! На услуги стоимостью 10 000 руб. и меньше скидки не распространяются!
							.row-btn--center
								.modal-sale-get.modal-get(data-modal="order") Заказать
				.modal(data-modal="igs-1").modal--sale
					.modal-content
						.modal-close
							+icon("close","icon")
						.modal-sale__title Информация
						.modal-info
							p В уведомлении указываются такие сведения, как:
							ol
								li ФИО, паспортные данные, адрес застройщика, контактные данные для связи с застройщиком
								li адрес объекта, кадастровый номер земельного участка
								li сведения о правах застройщика на земельный участок
								li сведения о виде разрешенного использования земельного участка
								li сведения о планируемых параметрах объекта ИЖС или садового дома (в т.ч. об отступах от границ земельного участка),
								li сведения о том, что объект ИЖС или садовый дом не предназначен для раздела на самостоятельные объекты недвижимости.
							p К уведомлению необходимо приложить следующие документы:
							ol
								li правоустанавливающие документы на земельный участок
								li описание внешнего облика объекта ИЖС или садового дома (в случае, если строительство или реконструкция объекта ИЖС или садового дома планируется в границах территории исторического поселения федерального или регионального значения)
								li нотариальная доверенность (если документы подаются представителем застройщика).
				.modal(data-modal="igs-2").modal--sale
					.modal-content
						.modal-close
							+icon("close","icon")
						.modal-sale__title Информация
						.modal-info
							p К извещению об окончании строительства прилагаются следующие документы:
							ol
								li правоустанавливающие документы на земельный участок,
								li технический план объекта ИЖС или садового дома (в электронном виде на диске)
								li соглашение о распределении долей в праве общей собственности на объект ИЖС или садовый дом (в случае, если земельный участок принадлежит нескольким лицам на праве общей долевой собственности)
								li нотариальная доверенность (если документы подаются представителем застройщика).
							p.attanintion  <strong>ВАЖНО!:</strong> Технический план объекта подготавливается только аттестованным кадастровым инженером, входящим в СРО кадастровых инженеров и имеющим усиленную квалифицированную электронную подпись.
				.modal(data-modal="igs-3").modal--sale
					.modal-content
						.modal-close
							+icon("close","icon")
						.modal-sale__title Информация
						.modal-info
							p В Росреестр предоставляются следующие документы:
							ol
								li технический план объекта ИЖС или садового дома (в электронном виде на диске) с включенным в его состав полученным уведомлением о соответствии построенного/реконструированного объекта ИЖС или садового дом (для этого застройщику необходимо повторно обратиться к кадастровому инженеру)
								li нотариальная доверенность на подачу и получение документов из Росреестра (если документы подаются представителем собственника земельного участка, на котором построен объект)
								li квитанция об оплаченной гос.пошлине за регистрацию права (350 руб. для физ.лиц).
				.modal(data-modal="remove-1").modal--sale
					.modal-content
						.modal-close
							+icon("close","icon")
						.modal-sale__title Информация
						.modal-info
							p В уведомлении указываются такие сведения, как:
							ul
								li ФИО, паспортные данные, адрес застройщика, контактные данные для связи с застройщиком (для физ.лица),
								li наименование и место нахождения застройщика, регистрационные данные, контактные данные для связи (для юр.лица),
								li адрес объекта, кадастровый номер земельного участка,
								li сведения о правах застройщика на земельный участок,
								li сведения о правах застройщика на объект капитального строительства,
								li сведения о решении суда о необходимости сноса объекта капитального строительства либо о наличии обязательства по сносу самовольной постройки (при наличии таких решения либо обязательства).
							p К уведомлению необходимо приложить следующие документы:
							ul
								li результаты и материалы обследования объекта капитального строительства,
								li проект организации работ по сносу объекта капитального строительства,
								li нотариальная доверенность (если документы подаются представителем застройщика).
				.modal(data-modal="remove-2").modal--sale
					.modal-content
						.modal-close
							+icon("close","icon")
						.modal-sale__title Информация
						.modal-info
							p К уведомлению о завершении сноса объекта капитального строительства прилагаются следующие документы:
							ul
								li ФИО, паспортные данные, адрес застройщика, контактные данные для связи с застройщиком (для физ.лица),
								li наименование и место нахождения застройщика, регистрационные данные, контактные данные для связи (для юр.лица),
								li адрес объекта, кадастровый номер земельного участка,
								li сведения о правах застройщика на земельный участок,
								li сведения о правах застройщика на объект капитального строительства,
								li сведения о решении суда о необходимости сноса объекта капитального строительства либо о наличии обязательства по сносу самовольной постройки (при наличии таких решения либо обязательства).
							p К уведомлению необходимо приложить следующие документы:
							ul
								li документ, удостоверяющий личность заявителя,
								li правоустанавливающие документы на земельный участок,
								li правоустанавливающие документы на снесенный объект капитального строительства,
								li нотариальная доверенность (если документы подаются представителем застройщика).


//block footer end